{
  "name": "pburggraf/crc",
  "description": "Cyclic redundancy checker (crc) class to calculate crc8, crc16, crc24 and crc32 checksum values",
  "license": "MIT",
  "keywords": [
    "crc",
    "crc8",
    "crc16",
    "crc24",
    "crc32",
    "checksum",
    "validation",
    "checksum generator",
    "cyclic redundancy checker"
  ],
  "authors": [
    {
      "name": "Philip Burggraf",
      "email": "philip@pburggraf.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "PBurggraf\\CRC\\": "src/PBurggraf/CRC/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PBurggraf\\CRC\\Tests\\": "tests/PBurggraf/CRC/"
    }
  },
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-phpunit": "^1.0",
    "friendsofphp/php-cs-fixer": "^3.0"
  }
}
